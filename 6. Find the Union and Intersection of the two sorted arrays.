
import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int test = Integer.parseInt(sc.nextLine());
		
		while(test-- > 0){
		    
		    StringTokenizer str = new StringTokenizer(sc.nextLine());
		    int len1 = Integer.parseInt(str.nextToken());
		    int len2 = Integer.parseInt(str.nextToken());
		    
		    StringTokenizer str1 = new StringTokenizer(sc.nextLine());
		    StringTokenizer str2 = new StringTokenizer(sc.nextLine());
		    
		    Set<Integer> result = new HashSet<>();
		    while(str1.hasMoreTokens()){
		        
		        result.add(Integer.parseInt(str1.nextToken()));
		        
		       
		    }
		    
		    while(str2.hasMoreTokens()){
		        result.add(Integer.parseInt(str2.nextToken()));
		    }
		    
		    
		    System.out.println(result.size());
		    
		    
		    
		}
		
		
		
	}
}
